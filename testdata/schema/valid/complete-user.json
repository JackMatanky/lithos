{
  "name": "user",
  "extends": "base",
  "excludes": ["internal_id"],
  "properties": {
    "email": {
      "type": "string",
      "required": true,
      "pattern": "^[\\w\\.-]+@[\\w\\.-]+\\.[A-Za-z]{2,}$"
    },
    "age": {
      "type": "number",
      "required": false,
      "min": 0,
      "max": 150
    },
    "active": {
      "type": "bool",
      "required": true
    }
  }
}
